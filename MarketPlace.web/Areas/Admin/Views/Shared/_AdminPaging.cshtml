@model MarketPlace.dataLayer.DTOs.Paging.BasePaging
<ul class="pagination">
    <li class="@(Model.StartPage<Model.PageId?"":"disabled")"><a onclick="FillPageId(@(Model.PageId-1))">«</a></li>
    @for (int i = Model.StartPage; i <= Model.EndPage; i++)
    {
        <li class="@(Model.PageId==i?"active":"")"><a onclick="FillPageId(@i)">@i</a></li>
    }
    <li class="@(Model.EndPage<Model.PageId?"disabled":"")"><a onclick="FillPageId(@(Model.PageId+1))">»</a></li>
</ul>
@*@(Model.PageId == i ? "<span class='sr-only'>(current)</span>":"")*@