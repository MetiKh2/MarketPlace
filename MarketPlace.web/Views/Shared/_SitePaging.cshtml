@model MarketPlace.dataLayer.DTOs.Paging.BasePaging
<div class="shop_toolbar t_bottom">
	<div class="pagination">
		<ul>
			@for (var i = Model.StartPage; i <= Model.EndPage; i++)
		   {
			<li class="@(Model.PageId==i?"current":"")"><a onclick="FillPageId(@i)">@i</a></li>
		   }
		   @if (Model.PageId<Model.EndPage)
		  {
			<li class="next"><a onclick="FillPageId(@(Model.PageId+1))">بعدی</a></li>
			<li><a onclick="FillPageId(@(Model.PageId+1))"><i class="fa fa-angle-double-left"></i></a></li>
		  
		  }
			</ul>
	</div>
</div>