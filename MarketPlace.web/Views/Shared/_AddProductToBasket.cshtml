@model MarketPlace.dataLayer.DTOs.Order.AddProductToOrderDto
<form method="post" id="add-product-to-order-form" asp-area="User" asp-controller="Order" asp-action="AddProductToOrder"
data-ajax="true"
data-ajax-success="onSuccessAddProductToOrder"
data-ajax-method="post"
>
    <input type="hidden" id="add-product-to-order-productId" asp-for="ProductId"/>
    <input type="hidden" id="add-product-to-order-productColorId" asp-for="ProductColorId"/>
    <input type="hidden" id="add-product-to-order-count" asp-for="Count"/>
</form>
